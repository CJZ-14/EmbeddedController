/ {
	named-pwms {
		compatible = "named-pwms";

		pwm_fan: fan {
			pwms = <&pwm0 0 0>;
			label = "FAN";
			frequency = <25000>;
		};
	};
};

/* Fan control */
&pwm0 {
	status = "okay";
	drive-open-drain;
};
